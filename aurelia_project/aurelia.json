{
  "name": "taxi-app",
  "type": "project:application",
  "paths": {
    "root": "src",
    "resources": "resources",
    "elements": "resources/elements",
    "attributes": "resources/attributes",
    "valueConverters": "resources/value-converters",
    "bindingBehaviors": "resources/binding-behaviors"
  },
  "dependencies":[
    "velocity-animate",
    "tether",
    {
      "name": "aurelia-bootstrap",
      "path": "../node_modules/aurelia-bootstrap/dist/amd",
      "main": "index",
      "resources": [
        "**/*.html"
      ]
    }
  ],
  "transpiler": {
    "id": "typescript",
    "fileExtension": ".ts"
  },
  "build": {
    "options": {
      "server": "dev",
      "extractCss": "prod",
      "coverage": false
    }
  },
  "platform": {
    "hmr": false,
    "open": false,
    "port": 9000,
    "output": "dist"
  },
  "packageManager": "npm"
}
